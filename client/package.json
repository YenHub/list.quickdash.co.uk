  "scripts": {
    "start": "react-scripts start",
    "start:beta": "cross-env REACT_APP_BETA=true react-scripts start",
    "build": "react-scripts build",
    "build:beta": "cross-env REACT_APP_BETA=true react-scripts build",
    "test": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write **/*.{ts,tsx}",
    "refresh:staging": "git checkout staging && npm run stash:staging",
    "stash:staging": "git stash save -k -u -m \"Staging Stash - we all make mistakes :o)\" && npm run git:reset",
    "git:reset": "git reset --hard && npm run pull:master",
    "pull:master": "git checkout master && git fetch -p && git pull --all && npm run branch:staging",
    "branch:staging": "git branch -D staging && git branch staging && npm run publish:staging",
    "publish:staging": "git push -u origin staging"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
